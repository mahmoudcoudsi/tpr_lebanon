            <div class="container">
                <div class="navbar">
                    <div class="navbar-inner">
                        <div class="brand-wrapper">
                          <a class="brand" href="/"><img src="../images/reviewlogo.png" class="logo"></a>
                        </div>
                          <ul class="nav">
                            <li><%= link_to "SAT", '/sat/index' %></li>
                            <li><%= link_to "TOEFL", '/home/soon' %></li>
                            <li><%= link_to "GMAT", '/home/soon' %></li>
                            <li><%= link_to "GRE", '/home/soon' %></li>
                            <li><%= link_to "MCAT", '/home/soon' %></li>
                            <li><%= link_to "USMLE", '/home/soon' %></li>
                          </ul>
                    </div>
                </div>
            </div>
        </header>
        <!-- End header -->
        <!-- Start page header -->              
        <!-- End page header -->   
        <!-- Start Yield -->
            <div class="container">
                <div class="row">
                    <div class="span12">
                      <%= yield %>
                    </div>
                </div>
            </div>
        <!-- End Yield -->
        <!-- Start socket -->
        <section class="socket">
            <div class="container">
                <div class="row">
                    <div class="span6">
                        <span>Â© 2013 The Princeton Revirew Lebanon. All rights reserved. Authorized Users only.</span>     
                    </div>
                </div>     
            </div>
        </section>
      <%= yield :scripts %>
        <!-- End socket -->
        <!-- Le javascript
        ================================================== -->
